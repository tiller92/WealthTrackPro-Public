generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int    @id @default(autoincrement())
  first_name String
  last_name  String
  email      String
  username   String @unique
  password   String
  stocks Userstocks[]
}

model Userstocks {
  id  Int @id @default(autoincrement())
  ticker String
  shares Int
  owner User @relation(fields: [ownerName],references: [username])
  ownerName String
}
